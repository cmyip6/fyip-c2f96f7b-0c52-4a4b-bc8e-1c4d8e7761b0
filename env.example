NODE_ENV=development

## drop schema: boolean
DROP_SCHEMA=false
## run migrations: boolean
RUN_MIGRATIONS=true
## run seed: boolean 
RUN_SEEDS=false 

API_PORT=8001
API_HOST=localhost
API_PROTOCOL=http

DB_PORT=5432
DB_HOST=localhost

# default db for demo purpose
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
